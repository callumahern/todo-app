<div class="card-header d-flex justify-content-between">
  <span>TodoLists</span>
  <%= link_to "New todo list", new_todo_list_path %>
</div>

<ul class="list-group">
    <% @todo_lists.each do |todo_list| %>
      <li class="list-group-item">
        <%= link_to todo_list.title, todo_list %>
      </li>
    <% end %>
  </div>
</ul>
